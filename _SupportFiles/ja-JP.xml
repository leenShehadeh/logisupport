<DefinitionModifier>
	<SetAttributeWithInsert ID="report-header" Class="japanese" />
	<SetAttributeWithInsert XPath="//Body" Class="japanese" />
	
	<SetAttributeWhenEmpty XPath="//ChartCanvas" NoDataCaption="表示するデータがありません" />
	<SetAttribute XPath="//ResetZoomButton[@Caption='Reset Zoom']" Caption="ズームをリセット" />

	<SetAttribute XPath="//Label[@Caption='My Chart To Export to PDF']" Caption="PDFにエクスポートする私のチャート" />
	<SetAttribute XPath="//ChartCanvas[@ID='myChart']/ChartXAxis/AxisLabelStyle" Format="M月d日" />
	<SetAttribute XPath="//Series[@LegendLabel='Live']" LegendLabel="住むABCDE." />
	<SetAttribute XPath="//Series[@LegendLabel='On Demand']" LegendLabel="オンデマンドABCDE." />
	
	<SetAttribute XPath="//Button[@Caption='Export To PDF']" Caption="にエクスポート." />
	
	<SetAttribute XPath="//StaticDataRow[@REGLABEL='Live']" REGLABEL="住む." />
	<SetAttribute XPath="//StaticDataRow[@REGLABEL='On Demand']" REGLABEL="オンデマンド" />
	<SetAttribute XPath="//StaticDataRow[@REGLABEL='None']" REGLABEL="無し" />
	<SetAttribute XPath="//StaticDataRow[@REGLABEL='Both']" REGLABEL="両方" />
	
	<!-- Benchmarking Industry PDF: Category -->
	<SetAttribute ID="images-benchmark-industry-category" Caption="images.benchmark_industry.category-ja.png" Height="" Width="50" />
	<SetAttribute XPath="//Label[@Caption='Metrics by Webinar Category']" Caption="ウェビナーカテゴリー別指標" />
	<SetAttribute XPath="//ChartCanvas[@ChartCaption='Engagement Score']" ChartCaption="エンゲージメントスコア" />
	<SetAttribute XPath="//ChartCanvas[@ChartCaption='Events']" ChartCaption="のイベント" />
	<SetAttribute XPath="//ChartCanvas[@ChartCaption='Registrants']" ChartCaption="登録者" />
	<SetAttribute XPath="//ChartCanvas[@ChartCaption='Attendees']" ChartCaption="参加者" />
	<SetAttribute XPath="//StaticDataRow[@LABEL='Marketing']" LABEL=" マーケティング " />
	<SetAttribute XPath="//StaticDataRow[@LABEL='Communications']" LABEL="コミュニケーション " />
	<SetAttribute XPath="//StaticDataRow[@LABEL='Continuing Education']" LABEL=" 社会人教育 " />
	<SetAttribute XPath="//StaticDataRow[@LABEL='Training/Learning']" LABEL=" トレーニング/学習 " />
	<SetAttribute XPath="//StaticDataRow[@LABEL='Other']" LABEL=" その他 " />


</DefinitionModifier>